@{
    ViewBag.Title = "Calculator8";
}
    <section class="ui-widget-content calc pure-g-r" id="calcCompare" style="text-align: center;">
        <section class="pure-u-1 left">
            <section class="inner">
                <section style="display: block; border-radius: 4px; background-color: #f3f3f3; padding: 20px; margin-bottom: 20px">
                    This calculator will allow you to compute up to three loans and compare the results.
                </section>
                <h1>Loan #1</h1>
                <section id="f">
                    <section class="pure-u-1-2 center-phone right">
                        <section class="inner">
                            Mortgage Term
                        </section>
                    </section>
                    <section class="pure-u-1-2 center-phone left">
                        <section class="inner">
                            <select id="term" class="pure-u-4-5">
                                <option value="" selected></option>
                                <option value="15">15</option>
                                <option value="30">30</option>
                            </select>
                        </section>
                    </section>
                    <section class="pure-u-1-2 center-phone right">
                        <section class="inner">
                            Interest Rate
                        </section>
                    </section>
                    <section class="pure-u-1-2 center-phone left">
                        <section class="inner">
                            <input name="interest" class="style9" style="width: 60%" id="interest" size="4">
                            %
                        </section>
                    </section>
                    <section class="pure-u-1-2 center-phone right">
                        <section class="inner">
                            Loan Amount
                        </section>
                    </section>
                    <section class="pure-u-1-2 center-phone left">
                        <section class="inner">
                            <input name="principal" class="style9 pure-u-4-5" id="principal" size="10">
                        </section>
                    </section>
                    <section class="pure-u-1-2 center-phone right">
                        <section class="inner">
                        </section>
                    </section>
                    <section class="pure-u-1-2 center-phone left">
                        <section class="inner">
                            <input name="button" type="button" class="Calc_Button" onclick="computeForm($('#f'))" value="Compute">
                            <input name="reset" type="reset" class="Calc_Button" value="Reset">
                        </section>
                    </section>
                    <section class="pure-u-1-2 center-phone right">
                        <section class="inner">
                            Monthly Payment for Loan #1
                        </section>
                    </section>
                    <section class="pure-u-1-2 center-phone left">
                        <section class="inner">
                            <input name="payment" class="style9 pure-u-4-5" id="payment" onchange="computeField(this)" size="10">
                        </section>
                    </section>
                </section>
                <h1>Loan #2</h1>
                <section id="b">
                    <aside class="pure-u-1-2 center-phone right">
                        <aside class="inner">
